{
    "dev": {
        "aws_region": "sa-east-1",
        "django_settings": "brscans.settings",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "brscans",
        "project_name": "brscans-backend",
        "runtime": "python3.12",
        "s3_bucket": "brscans-zappa",
        "environment_variables": {
            "CLOUDFRONT_DOMAIN": "d47sf0ah3vbtc.cloudfront.net",
            "PRODUCTION": "True",
            "S3_STATIC_BUCKET": "brscans-media",
            "S3_REGION": "sa-east-1"
        },
        "timeout_seconds": 900,
        "async_source": "sns",
        "async_resources": true,
        "memory_size": 1536
    }
}